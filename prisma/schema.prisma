generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id       String   @id @default(cuid())
  email    String   @unique
  name     String
  profileImage String?

  createdAt        DateTime @default(now())
  updatedAt        DateTime @updatedAt
}



